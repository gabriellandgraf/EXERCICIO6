algoritmo "números"


var
numero: real
antecessor: real
sucessor: real

inicio
 escreva (" digite um numero: ")
 leia (numero)
 
 antecessor<- numero -1
 sucessor<- numero +1
 
 escreval (" o numero ", numero, " tem como antecessor o numero ", antecessor, " e como sucessor o numero", sucessor, ".")

fimalgoritmo
